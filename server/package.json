{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start:ci": "ts-node ./bin/www.ts",
    "dev:ci": "DEBUG=* ts-node ./bin/www.ts",
    "lint": "eslint . --ext .ts",
    "clean:samples": "node ./scripts/clean_samples.js",
    "smoke:export": "./scripts/run_smoke_export.sh",
    "verify-export": "bash ./scripts/smoke-export.sh && node ./scripts/extract-pdf-text.js",
    "verify-export:inproc": "node ./scripts/run_export_test_inproc.js",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "worker": "node worker-sqlite.mjs",
    "test": "vitest",
    "test:export": "vitest run ./__tests__/export_text.test.mjs --reporter dot",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "e2e:smoke": "node ./scripts/e2e-smoke.js"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "puppeteer-core": "^24.16.2",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "undici": "^6.2.5",
    "uuid": "^11.1.0"
  },
  "optionalDependencies": {
    "bullmq": "^1.91.1",
    "ioredis": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.10",
    "pdfjs-dist": "^5.4.54",
    "prisma": "^6.14.0",
    "supertest": "^7.1.3",
    "typescript": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
